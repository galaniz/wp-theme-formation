{
  "name": "wp-theme-formation",
  "version": "1.0.0",
  "description": "Provide base, utilities and added functionality for building WordPress themes",
  "main": "index.js",
  "scripts": {
    "build": "webpack --optimize-minimize -p",
    "watch": "webpack --optimize-minimize -p --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galaniz/wp-theme-foundation.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/galaniz/wp-theme-foundation/issues"
  },
  "homepage": "https://github.com/galaniz/wp-theme-foundation#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "cssnano": "^4.1.10",
    "fibers": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 3 versions",
              "ie >= 10"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ]
    ]
  },
  "browserslist": [
    "> 1%",
    "last 20 versions"
  ],
  "dependencies": {
    "core-js": "^3.6.1",
    "css-loader": "^1.0.1",
    "css-mqpacker": "^7.0.0",
    "dompurify": "^2.0.7",
    "postcss-combine-duplicated-selectors": "^8.1.0"
  }
}
